{
    "name": "XPT",
    "description": "XRPL Portfolio Tracker Browser extension fetches information about your XRPL accounts",
    "version": "1.1.2",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "XPT"
    },
    "icons": {
        "16": "xpt192.png",
        "48": "xpt192.png",
        "128": "xpt192.png"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'"
}
