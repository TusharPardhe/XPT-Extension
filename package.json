{
    "name": "xrpl-browser-extension",
    "version": "1.1.4",
    "private": true,
    "eslintConfig": {
        "env": {
            "webextensions": true
        },
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
