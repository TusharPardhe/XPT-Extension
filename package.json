        "axios": "^0.26.0",
        "bulma": "^0.9.3",
        "crypto-browserify": "^3.12.0",
        "crypto-js": "^4.2.0",
        "dateformat": "^5.0.3",
        "events": "^3.3.0",
        "framer-motion": "^10.18.0",
        "html-react-parser": "^3.0.1",
        "https-browserify": "^1.0.0",
        "material-design-icons": "^3.0.1",
